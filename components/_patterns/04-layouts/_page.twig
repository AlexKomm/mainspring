<div class="l-page">
  <div class="l-page__header">
    {% include "@organisms/site/00-header/header.twig"
      with {
        logo: drupal_block('mainspring_branding')
      }
    %}
  </div>

  {{ content }}

  <div class="l-page__footer">
    {% set year = 'now'|date('Y') %}

    {% include "@organisms/site/01-footer/footer.twig"
      with {
        footer_logo: drupal_block('mainspring_powered'),
        year: year
      }
    %}
  </div>
</div>
